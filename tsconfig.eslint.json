{
    "extends": "./tsconfig.json",
    "include": [
      "./**/*.ts",
      "./**/*.tsx",
      "./**/*.js",
      "./**/*.cjs",
      "tailwind.config.js",
      ".eslintrc.cjs"
    ],
    "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@renderer/*": [
        "src/renderer/src/*"
      ],
      "@/*": ["src/renderer/src/*"],
      "src/*": ["src/renderer/src/*"],
      "shared/*": ["src/shared/*"]

    }
  }
  }
  